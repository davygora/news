<h3>News from news.ycombinator.com</h3>
 
<table>
  <tr>
    <th>Id</th>
    <th>Title</th>
    <th>Url</th>
    <th>Author</th>
  </tr>
 
  <% @articles.each do |article| %>
    <tr>
      <td><%= article.id %></td>
      <td><%= article.title %></td>
      <td><a href="<%= article.url %>"><%= article.url %></td>
      <td><%= article.author %></td>
    </tr>
  <% end %>
</table>
<%= will_paginate @articles %>

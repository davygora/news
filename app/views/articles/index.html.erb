<h3>News from news.ycombinator.com</h3>
 
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th>Url</th>
  </tr>
 
  <% @articles.each do |article| %>
    <tr>
      <td><%= article.title %></td>
      <td><%= article.text %></td>
      <td><%= article.URL %></td>
    </tr>
  <% end %>
</table>
